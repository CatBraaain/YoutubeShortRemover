{
    "name": "Youtube Shorts Remover",
    "version": "1.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "run_at": "document_start",
            "css": [
                "youtube-shorts-remover.css"
            ]
        }
    ],
    "homepage_url": "https://github.com/CatBraaain/chrome-youtube-shorts-remover"
}
